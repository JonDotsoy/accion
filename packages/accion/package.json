{
  "name": "accion",
  "description": "Streamline script execution with a versatile module: execute JS or shell scripts as 'jobs,' manage dependencies, and monitor progress effortlessly",
  "version": "0.3.3",
  "type": "module",
  "license": "MIT",
  "types": "job.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./lib/types/job.d.ts",
      "import": "./lib/esm/job.js",
      "default": "./lib/cjs/job.js"
    },
    "./job": {
      "types": "./lib/types/job.d.ts",
      "import": "./lib/esm/job.js",
      "default": "./lib/cjs/job.js"
    },
    "./dsn": {
      "types": "./lib/types/dsn.d.ts",
      "import": "./lib/esm/dsn.js",
      "default": "./lib/cjs/dsn.js"
    },
    "./expose": {
      "types": "./lib/types/expose.d.ts",
      "import": "./lib/esm/expose.js",
      "default": "./lib/cjs/expose.js"
    },
    "./manager": {
      "types": "./lib/types/manager.d.ts",
      "import": "./lib/esm/manager.js",
      "default": "./lib/cjs/manager.js"
    }
  },
  "devDependencies": {
    "bun-types": "^1.0.18",
    "glob": "^10.3.10",
    "prettier": "^3.2.4",
    "use-workspace": "^0.1.5",
    "yaml": "^2.3.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "make build",
    "fmt": "prettier -w ."
  },
  "files": [
    "./lib/cjs",
    "./lib/esm",
    "./lib/types",
    "./src"
  ],
  "dependencies": {
    "@accions/common": "^0.2.4",
    "nanostores": "^0.9.5"
  }
}
