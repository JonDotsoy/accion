{
  "name": "accion",
  "description": "Streamline script execution with a versatile module: execute JS or shell scripts as 'jobs,' manage dependencies, and monitor progress effortlessly",
  "version": "0.1.21",
  "type": "module",
  "license": "MIT",
  "main": "./src/job.js",
  "module": "./src/job.js",
  "types": "./src/job.d.ts",
  "exports": {
    ".": {
      "import": "./src/job.js",
      "types": "./src/job.d.ts"
    },
    "./job": {
      "import": "./src/job.js",
      "types": "./src/job.d.ts"
    },
    "./src/job": {
      "import": "./src/job.js",
      "types": "./src/job.d.ts"
    },
    "./expose": {
      "import": "./src/expose.js",
      "types": "./src/expose.d.ts"
    },
    "./src/expose": {
      "import": "./src/expose.js",
      "types": "./src/expose.d.ts"
    },
    "./manager": {
      "import": "./src/manager.js",
      "types": "./src/manager.d.ts"
    },
    "./src/manager": {
      "import": "./src/manager.js",
      "types": "./src/manager.d.ts"
    }
  },
  "devDependencies": {
    "bun-types": "^1.0.18",
    "glob": "^10.3.10",
    "yaml": "^2.3.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "bun scripts/build.ts"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "@accions/common": "^0.1.5"
  }
}
