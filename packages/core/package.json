{
  "name": "accion",
  "description": "Streamline script execution with a versatile module: execute JS or shell scripts as 'jobs,' manage dependencies, and monitor progress effortlessly",
  "version": "v0.1.3",
  "module": "./dist/job.js",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/job.js",
      "types": "./src/job.ts"
    },
    "./job": {
      "import": "./dist/job.js",
      "types": "./src/job.ts"
    },
    "./expose": {
      "import": "./dist/expose.js",
      "types": "./src/expose.ts"
    },
    "./manager": {
      "import": "./dist/manager.js",
      "types": "./src/manager.ts"
    }
  },
  "devDependencies": { "bun-types": "^1.0.18" },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "bun build src/job.ts src/manager.ts src/expose.ts --target node --outdir dist"
  },
  "dependencies": {
    "debug": "^4.3.4"
  }
}